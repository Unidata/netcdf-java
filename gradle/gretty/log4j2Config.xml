<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="" packages="">
  <Appenders>
    <!--
      Console
      -->
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%d{yyyy-MM-dd' 'HH:mm:ss Z}] %-5p [%c:%L] - %m%n"/>
    </Console>
  </Appenders>
  <Loggers>

    <!-- All logging messages go to the console -->
    <Root level="warn"
      additivity="false">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>